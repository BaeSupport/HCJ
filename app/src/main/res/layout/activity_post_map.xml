<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.postmap.PostMapActivity">

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/postmap"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Yuseonggu"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="220dp"
        android:src="@drawable/yeo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Daeduckgu"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_marginStart="184dp"
        android:layout_marginTop="188dp"
        android:src="@drawable/dae"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Donggu"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginStart="212dp"
        android:layout_marginTop="284dp"
        android:src="@drawable/dong"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Junggu"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginStart="152dp"
        android:layout_marginTop="328dp"
        android:src="@drawable/jung"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Sugu"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="352dp"
        android:src="@drawable/su"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <android.support.design.widget.BottomNavigationView
        android:id="@+id/mainBottomNavigation"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        app:itemIconTint="@color/bottom_sheet_color"
        app:itemTextColor="@color/bottom_sheet_color"
        app:layout_constraintBottom_toBottomOf="parent"
        app:menu="@menu/bottom_navigation_menu" />

</android.support.constraint.ConstraintLayout>