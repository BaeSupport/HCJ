<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/postmap"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/Yuseonggu"
        android:layout_width="100dp"
        android:layout_height="30dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="240dp"
        android:src="@drawable/yeo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Daeduckgu"
        android:layout_width="70dp"
        android:layout_height="20dp"
        android:layout_marginStart="188dp"
        android:layout_marginTop="220dp"
        android:src="@drawable/dae"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Donggu"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginStart="208dp"
        android:layout_marginTop="292dp"
        android:src="@drawable/dong"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Junggu"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginStart="144dp"
        android:layout_marginTop="332dp"
        android:src="@drawable/jung"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/Sugu"
        android:layout_width="70dp"
        android:layout_height="30dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="360dp"
        android:src="@drawable/su"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</android.support.constraint.ConstraintLayout>